doi = {10.1016/j.euromechflu.2021.01.007},
file = {:Users/ppiper/Documents/mendeley/European Journal of Mechanics, BFluids/Sang et al.{\_}2021{\_}A data-driven artificial neural network model for predicting wind load of buildings using GSM-CFD solver.pdf:pdf;:Users/ppiper/Documents/mendeley/European Journal of Mechanics, BFluids/Sang et al.{\_}2021{\_}A data-driven artificial neural network model for predicting wind load of buildings using GSM-CFD solver(2).pdf:pdf},
issn = {09977546},
