@book{Kutz2021,
abstract = {We demonstrate that the integration of the recently developed dynamic mode decomposition (DMD) with a multi-resolution analysis allows for a decomposition method capable of robustly separating complex systems into a hierarchy of multi-resolution time-scale components. A one-level separation allows for background (low-rank) and foreground (sparse) separation of dynamical data, or robust principal component analysis. The multi-resolution dynamic mode decomposition is capable of characterizing nonlinear dynamical systems in an equation-free manner by recursively decomposing the state of the system into low-rank terms whose temporal coefficients in time are known. DMD modes with temporal frequencies near the origin (zero-modes) are interpreted as background (low-rank) portions of the given dynamics, and the terms with temporal frequencies bounded away from the origin are their sparse counterparts. The multi-resolution dynamic mode decomposition (mrDMD) method is demonstrated on several examples involving multi-scale dynamical data, showing excellent decomposition results, including sifting the El Ni$\backslash${\~{}}no mode from ocean temperature data. It is further applied to decompose a video data set into separate objects moving at different rates against a slowly varying background. These examples show that the decomposition is an effective dynamical systems tool for data-driven discovery.},
author = {Kutz, J Nathan},
file = {:Users/ppiper/Documents/mendeley/Unknown/Kutz{\_}2021{\_}Dynamic Mode Decomposition{\_} Data-Driven Modeling of Complex Systems-SIAM-Society for Industrial and Applied Mathematics.pdf.pdf:pdf},
isbn = {9781611974492},
pages = {234},
title = {{Dynamic Mode Decomposition{\_} Data-Driven Modeling of Complex Systems-SIAM-Society for Industrial and Applied Mathematics.pdf}},
year = {2021}
}
@article{Yasuda2021,
abstract = {Porous materials are key components of various applications. X-ray micro-computed tomography provides digitized sub-micron-scale 3D images of porous materials. Compared to conventional experimental characterizations, more structural features can be easily quantified using such 3D images in terms of geometrical descriptors. Therefore, we propose a machine learning and data-driven framework to identify an optimal descriptor subset for advanced characterization and accurate performance prediction of a porous material in a given application. Thus, targeted processing of the porous material can be realized. We investigate the permeability of porous materials for demonstration purposes. The descriptor selection step consists of two methods. The first method is the filter method, which involves importance ranking after inter-descriptor correlation-based instant screening from the entire set of descriptors using RReliefF and Kendall's tau. The second method is the wrapper method, which involves prediction-accuracy-based one-by-one selection and withdrawal from the previously screened descriptors using a support vector machine. Seven distinctive types of 528 porous media were digitally prepared, and their permeabilities were determined via CFD simulations. A total of 154 descriptors were evaluated via image processing. The proposed framework identified 11 descriptors and achieved more accurate prediction of the permeability compared to the existing correlations. Further, in the examined permeability range, the descriptors related to the pore size distribution allowed detection of flow channeling. These results effectively demonstrate the potential of the framework for the design and processing of porous materials by predicting their performance and hindered characteristics (e.g., channeling defects) from the identified descriptor subset in any application.},
author = {Yasuda, Tomoki and Ookawara, Shinichi and Yoshikawa, Shiro and Matsumoto, Hideyuki},
doi = {10.1016/j.cej.2021.130069},
file = {:Users/ppiper/Documents/mendeley/Chemical Engineering Journal/Yasuda et al.{\_}2021{\_}Machine learning and data-driven characterization framework for porous materials Permeability prediction and channeli.pdf:pdf},
issn = {13858947},
journal = {Chemical Engineering Journal},
keywords = {,Computational fluid dynamics,Feature selection,Machine learning,Porous material,Structural characterization},
month = {sep},
publisher = {Elsevier B.V.},
title = {{Machine learning and data-driven characterization framework for porous materials: Permeability prediction and channeling defect detection}},
volume = {420},
year = {2021}
}
@article{Hu2020,
abstract = {The solution of how to accurately and timely predict the flowrate of gas-liquid mixtures is the key to help petroleum and other related industries to reduce costs, improve efficiency, and optimize management. Although numerous studies have been carried out over the past decades, the problem is still significantly challenging due to the complexity of multiphase flows. This paper attempts to seek new possibilities for multiphase flow measurement and novel application scenarios for state-of-the-art machine learning (ML) techniques. Convolutional neural networks (CNNs) are applied to predict the flowrate of multiphase flows for the first time and can achieve promising performance. In addition, considering the difference between data distributions of training and testing samples and its negative impact on prediction accuracy of the CNN models on testing samples, we propose flow adversarial networks (FANs) that can distill both domain-invariant and flowrate-discriminative features from the raw input. The method is evaluated on dynamic experimental data of different multiphase flows on different flow conditions and operating environments. The experimental results demonstrate that FANs can effectively prevent the accuracy degradation caused by the gap between training and testing samples and have better performance than state-of-the-art approaches in the flowrate prediction field.},
