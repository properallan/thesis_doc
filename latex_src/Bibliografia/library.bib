doi = {10.1016/j.compbiomed.2022.105384},
eprint = {2201.03030},
file = {:Users/ppiper/Documents/mendeley/Computers in Biology and Medicine/Groun et al.{\_}2022{\_}Higher order dynamic mode decomposition From fluid dynamics to heart disease analysis.pdf:pdf},
issn = {18790534},
journal = {Computers in Biology and Medicine},
keywords = {Data-driven methods,Echocardiography,HODMD,Medical imaging},
