author = {Pawar, Suraj and San, Omer},
doi = {10.1103/PhysRevFluids.6.050501},
eprint = {2006.08901},
file = {:Users/ppiper/Documents/mendeley/Physical Review Fluids/Pawar, San{\_}2021{\_}Data assimilation empowered neural network parametrizations for subgrid processes in geophysical flows.pdf:pdf;:Users/ppiper/Documents/mendeley/Physical Review Fluids/Pawar, San{\_}2021{\_}Data assimilation empowered neural network parametrizations for subgrid processes in geophysical flows(2).pdf:pdf},
issn = {2469990X},
journal = {Physical Review Fluids},
language = {en},
month = {mar},
