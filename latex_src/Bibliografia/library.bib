@article{Ubaru2021,
abstract = {In this study, we address three important challenges related to disease transmissions such as the COVID-19 pandemic, namely, (a) providing an early warning to likely exposed individuals, (b) identifying individuals who are asymptomatic, and (c) prescription of optimal testing when testing capacity is limited. First, we present a dynamic-graph based SEIR epidemiological model in order to describe the dynamics of the disease propagation. Our model considers a dynamic graph/network that accounts for the interactions between individuals over time, such as the ones obtained by manual or automated contact tracing, and uses a diffusion–reaction mechanism to describe the state dynamics. This dynamic graph model helps identify likely exposed/infected individuals to whom we can provide early warnings, even before they display any symptoms and/or are asymptomatic. Moreover, when the testing capacity is limited compared to the population size, reliable estimation of individual's health state and disease transmissibility using epidemiological models is extremely challenging. Thus, estimation of state uncertainty is paramount for both eminent risk assessment, as well as for closing the tracing-testing loop by optimal testing prescription. Therefore, we propose the use of arbitrary Polynomial Chaos Expansion, a popular technique used for uncertainty quantification, to represent the states, and quantify the uncertainties in the dynamic model. This design enables us to assign uncertainty of the state of each individual, and consequently optimize the testing as to reduce the overall uncertainty given a constrained testing budget. These tools can also be used to optimize vaccine distribution to curb the disease spread when limited vaccines are available. We present a few simulation results that illustrate the performance of the proposed framework, and estimate the impact of incomplete contact tracing data.},
author = {Ubaru, Shashanka and Horesh, Lior and Cohen, Guy},
doi = {10.1016/j.jbi.2021.103901},
file = {:Users/ppiper/Documents/mendeley/Journal of Biomedical Informatics/Ubaru, Horesh, Cohen{\_}2021{\_}Dynamic graph and polynomial chaos based models for contact tracing data analysis and optimal testing prescrip.pdf:pdf},
issn = {15320464},
journal = {Journal of Biomedical Informatics},
keywords = {Contact tracing data,Covid-19 transmission,Dynamic graphs,Optimal testing prescription,Polynomial Chaos Expansion,Probabilistic SEIR model},
month = {oct},
publisher = {Academic Press Inc.},
title = {{Dynamic graph and polynomial chaos based models for contact tracing data analysis and optimal testing prescription}},
volume = {122},
year = {2021}
}
@article{Strofer2019c,
abstract = {Feature identification is an important task in many fluid dynamics applications and diverse methods have been developed for this purpose. These methods are based on a physical understanding of the underlying behavior of the flow in the vicinity of the feature. Particularly, they require the definition of suitable criteria (i.e. point-based or neighborhood-based derived properties) and proper selection of thresholds. However, these methods rely on creative visualization of physical idiosyncrasies of specific features and flow regimes, making them non-universal and requiring significant effort to develop. Here we present a physics-based, data-driven method capable of identifying any flow feature it is trained to. We use convolutional neural networks, a machine learning approach developed for image recognition, and adapt it to the problem of identifying flow features. This provides a general method and removes the large burden placed on identifying new features. The method was tested using mean flow fields from numerical simulations, where the recirculation region and boundary layer were identified in several two-dimensional flows through a convergent-divergent channel, and the horseshoe vortex was identified in three-dimensional flow over a wing-body junction.},
author = {Str{\"{o}}fer, Carlos Michel{\'{e}}n and Wu, Jin Long and Xiao, Heng and Paterson, Eric},
doi = {10.4208/CICP.OA-2018-0035},
file = {:Users/ppiper/Documents/mendeley/Communications in Computational Physics/Str{\"{o}}fer et al.{\_}2019{\_}Data-driven, physics-based feature extraction from fluid flow fields using convolutional neural networks(2).pdf:pdf;:Users/ppiper/Documents/mendeley/Communications in Computational Physics/Str{\"{o}}fer et al.{\_}2019{\_}Data-driven, physics-based feature extraction from fluid flow fields using convolutional neural networks.pdf:pdf},
issn = {19917120},
journal = {Communications in Computational Physics},
keywords = {,Convolutional neural network,Feature identification,Fluid dynamics,Machine learning},
number = {3},
pages = {625--650},
publisher = {Global Science Press},
title = {{Data-driven, physics-based feature extraction from fluid flow fields using convolutional neural networks}},
volume = {25},
year = {2019}
}
@article{Talaei2018,
abstract = {In this paper, an adaptive dynamic programming-based near optimal boundary controller is developed for partial differential equations (PDEs) modeled by the uncertain Burgers' equation under Neumann boundary condition in 2-D. Initially, Hamilton-Jacobi-Bellman equation is derived in infinite-dimensional space. Subsequently, a novel neural network (NN) identifier is introduced to approximate the nonlinear dynamics in the 2-D PDE. The optimal control input is derived by online estimation of the value function through an additional NN-based forward-in-time estimation and approximated dynamic model. Novel update laws are developed for estimation of the identifier and value function online. The designed control policy can be applied using a finite number of actuators at the boundaries. Local ultimate boundedness of the closed-loop system is studied in detail using Lyapunov theory. Simulation results confirm the optimizing performance of the proposed controller on an unstable 2-D Burgers' equation.},
