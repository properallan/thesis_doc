file = {:Users/ppiper/Documents/mendeley/Energy Conversion and Management/Zhang et al.{\_}2022{\_}Three-dimensional wind velocity reconstruction based on tensor decomposition and CFD data with experimental verificati.pdf:pdf},
issn = {01968904},
journal = {Energy Conversion and Management},
keywords = {Computational fluid dynamics,Fourth-order tensor database,Three-dimensional velocity distributions,Tucker decomposition,Wind field reconstruction},
month = {mar},
publisher = {Elsevier Ltd},
title = {{Three-dimensional wind velocity reconstruction based on tensor decomposition and CFD data with experimental verification}},
volume = {256},
year = {2022}
}
