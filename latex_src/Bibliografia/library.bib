keywords = {,Artificial neural network,Buildings,Drag coefficient,GSM-CFD,Rectangular cylinder,Wind load},
month = {may},
pages = {24--36},
publisher = {Elsevier Masson SAS.},
title = {{A data-driven artificial neural network model for predicting wind load of buildings using GSM-CFD solver}},
