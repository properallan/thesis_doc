keywords = {Data-driven model,POD algorithm,POD-BPNN surrogate model,POD-RBFNN surrogate model,Parallel twin cylinders},
number = {December 2023},
pages = {110342},
publisher = {Elsevier Ltd},
title = {{Flow fields prediction for data-driven model of parallel twin cylinders based on POD-RBFNN and POD-BPNN surrogate models}},
